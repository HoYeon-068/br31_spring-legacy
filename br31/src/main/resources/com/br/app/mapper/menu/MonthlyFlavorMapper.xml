<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.br.app.mapper.menu.MonthlyFlavorMapper">
  
 
    <select id="select" resultType="com.br.app.domain.menu.MonthlyFlavorDTO">
        SELECT *
        FROM (
            SELECT *
            FROM monthly_flavor
            ORDER BY monthly_flavor_id DESC
        )
        WHERE ROWNUM = 1
    </select>

  
</mapper>  