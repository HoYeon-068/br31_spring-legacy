<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.br.app.mapper.menu.ProductTagMapper">
  
  <select id="select"
        resultType="com.br.app.domain.menu.ProductTagDTO">
		SELECT *
		FROM product_tag
		WHERE products_id=#{products_num}

	</select>
	<insert id="insert">
		INSERT INTO product_tag
		VALUES (product_tag_seq.NEXTVAL,#{products_id},#{tag})
	</insert>



</mapper>  